{
  "parser": "babel-eslint",
  "extends": [
    "airbnb",
    "prettier",
    "prettier/react",
    "plugin:react-native/all",
    "plugin:jest/recommended"
  ],
  "plugins": ["prettier", "react", "jsx-a11y", "react-native", "jest"],
  "env": {
    "browser": true,
    "node": true,
    "es6": true,
    "react-native/react-native": true,
    "jest/globals": true
  },
  "parserOptions": {
    "ecmaVersion": 6,
    "sourceType": "module",
    "ecmaFeatures": {
      "jsx": true,
      "modules": true
    }
  },
  "rules": {
    "import/prefer-default-export": false,
    "prettier/prettier": "error",
    "global-require": 0,
    "react/jsx-filename-extension": [
      "error",
      { "extensions": [".js", ".jsx"] }
    ],
    "react/prop-types": 0,
    "react-native/no-inline-styles": 0,
    "react-native/no-color-literals": 0,
    "react-native/no-raw-text": ["error", { "skip": ["H1", "H2", "H3"] }],
    "react-native/sort-styles": 0
  }
}
